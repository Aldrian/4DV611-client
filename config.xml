<?xml version='1.0' encoding='utf-8'?>
<widget id="se.lnu.apd.client" version="0.0.1" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns="http://www.w3.org/ns/widgets">
  <name>Swedish Racetracks</name>
  <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
    Apache Cordova Team
  </author>
  <content src="index.html"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon density="xhdpi" height="57" src="res\android\mipmap-xhdpi\ic_launcher.png" width="57"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <engine name="ios" spec="~3.9.2"/>
  <engine name="android" spec="~4.1.1"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <plugin name="cordova-plugin-device" spec="~1.0.1"/>
  <plugin name="com.ionic.keyboard" spec="~1.0.4"/>
  <plugin name="cordova-plugin-splashscreen" spec="~2.1.0"/>
  <plugin name="onesignal-cordova-plugin" spec="~1.10.1"/>
  <plugin name="cordova-plugin-crosswalk-webview" spec="~1.2.0"/>
</widget>
